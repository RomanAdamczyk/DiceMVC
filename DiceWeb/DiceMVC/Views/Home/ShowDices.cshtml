@model Game

@{
    ViewData["Title"] = "ShowDices";
}

<h1>ShowDices</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>    
<table class="table">
    <thead>
        <tr>
            <th>

            </th>

        </tr>
    </thead>
    <tbody>
        @for (int i =0; i<5;i++)
        {
            {
                <tr>
                    <td>
                        @Model.Dices[i,0]
                    </td>
                </tr>
            }
        }
    </tbody>
</table>
